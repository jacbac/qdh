<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Articles" table="articles">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="dateparution" type="string" column="dateparution" length="255" nullable="false"/>
    <field name="journal" type="string" column="journal" length="255" nullable="false"/>
    <field name="journalid" type="integer" column="journalid" nullable="false"/>
    <field name="img" type="string" column="img" length="255" nullable="false"/>
    <field name="permalien" type="string" column="permalien" length="255" nullable="false"/>
    <field name="vignetteExport" type="text" column="vignette_export" nullable="false"/>
    <field name="description" type="text" column="description" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="false"/>
    <field name="changedOn" type="datetime" column="changed_on" nullable="false"/>
  </entity>
</doctrine-mapping>
