<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Chrono" table="chrono">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="datedebut" type="date" column="datedebut" nullable="false"/>
    <field name="datefin" type="date" column="datefin" nullable="false"/>
    <field name="mois" type="string" column="mois" length="255" nullable="false"/>
    <field name="annee" type="integer" column="annee" nullable="false"/>
    <field name="description" type="text" column="description" nullable="false"/>
    <field name="source" type="text" column="source" nullable="false"/>
    <field name="tags" type="integer" column="tags" nullable="false"/>
    <field name="createdOn" type="datetime" column="created_on" nullable="false"/>
    <field name="changedOn" type="datetime" column="changed_on" nullable="false"/>
  </entity>
</doctrine-mapping>
